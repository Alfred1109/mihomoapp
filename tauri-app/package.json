{
  "name": "mihomoapp",
  "version": "0.1.0",
  "description": "Mihomo management application built with Tauri, React, and MUI",
  "main": "frontend/main.tsx",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "prepare:resources": "node -e \"process.platform === 'win32' ? require('child_process').execSync('powershell -ExecutionPolicy Bypass -File prepare-resources.ps1', {stdio: 'inherit'}) : require('child_process').execSync('bash prepare-resources.sh', {stdio: 'inherit'})\"",
    "lint": "eslint frontend --ext .ts,.tsx",
    "lint:fix": "eslint frontend --ext .ts,.tsx --fix",
    "format": "prettier --write \"frontend/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"frontend/**/*.{ts,tsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "rust:fmt": "cd backend && cargo fmt",
    "rust:check": "cd backend && cargo clippy -- -D warnings",
    "check:all": "npm run type-check && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^5.16.10",
    "@mui/material": "^5.16.10",
    "@tauri-apps/api": "^1.6.0",
    "@tauri-apps/cli": "^1.6.3",
    "i18next": "^25.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.5.3",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vite": "^5.4.14"
  }
}
